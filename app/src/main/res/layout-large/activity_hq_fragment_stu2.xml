<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".uibase.HqFragmentStuActivity2"
    tools:ignore="MissingDefaultResource">

    <fragment
        android:id="@+id/hq_left_fragment"
        android:name="com.example.hqandroidstu.uibase.HqLeftFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        />

    <fragment
        android:id="@+id/hq_right_fragment"
        android:name="com.example.hqandroidstu.uibase.HqRightFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        />

</LinearLayout>