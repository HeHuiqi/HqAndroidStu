<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".uibase.HqListViewActivity">
    <com.example.hqandroidstu.uibase.HqCustomActionBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/hq_custom_action_bar2"
        />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/hq_custom_action_bar2"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/hq_list_view"
        />

</androidx.constraintlayout.widget.ConstraintLayout>